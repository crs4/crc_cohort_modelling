definition
	CLUSTER[at0000] matches {    -- Synoptic details - Colorectal cancer
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0330] occurrences matches {0..1} matches {    -- Neoadjuvant therapy given
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0331,    -- Yes
							at0332,    -- No
							at0333]    -- Not known
						}
					}
				}
			}
			allow_archetype CLUSTER[at0006] occurrences matches {0..*} matches {    -- Resection margin
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.tumour_resection_margins\.v0/}
			}
			CLUSTER[at0004] occurrences matches {0..1} matches {    -- Macroscopic findings
				items cardinality matches {1..*; unordered} matches {
					allow_archetype CLUSTER[at0005] occurrences matches {0..1} matches {    -- Tumour dimensions
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.physical_properties(-[a-zA-Z0-9_]+)*\.v0/}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {    -- Maximum extramuscular extension
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"mm">
										magnitude = <|>=0.0|>
									>
								>
							>
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {    -- Anastomotic doughnuts submitted
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
					CLUSTER[at0009] occurrences matches {0..1} matches {    -- Tumour perforation
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0010] occurrences matches {0..1} matches {    -- Tumour perforation
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0018,    -- Present
											at0019,    -- Absent
											at0420,    -- Equivocal
											at0017]    -- Indeterminate
										}
									}
								}
							}
							ELEMENT[at0011] occurrences matches {0..1} matches {    -- Perforation location
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0020,    -- Retro/infra peritoneal
											at0021]    -- Serosal
										}
									}
								}
							}
							ELEMENT[at0012] occurrences matches {0..1} matches {    -- Comment
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					CLUSTER[at0013] occurrences matches {0..1} matches {    -- For rectal tumours
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0014] occurrences matches {0..1} matches {    -- Relationship to anterior peritoneal reflection
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0022,    -- Entirely above
											at0023,    -- Astride
											at0024,    -- Entirely below
											at0372]    -- Indeterminate
										}
									}
								}
							}
							ELEMENT[at0015] occurrences matches {0..1} matches {    -- Intactness of the mesorectum
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0027,    -- Complete
											at0026,    -- Nearly complete
											at0025,    -- Incomplete
											at0028]    -- Indeterminate
										}
									}
								}
							}
						}
					}
					ELEMENT[at0373] occurrences matches {0..1} matches {    -- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
			CLUSTER[at0001] occurrences matches {0..1} matches {    -- Microscopic findings
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0033] occurrences matches {0..1} matches {    -- Histological tumour type
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0034,    -- Adenocarcinoma
									at0410,    -- Adenocarcinoma, cribriform comedo-type
									at0039,    -- Medullary carcinoma
									at0411,    -- Adenocarcinoma, micropapillary type
									at0035,    -- Mucinous adenocarcinoma
									at0412,    -- Adenocarcinoma, serrated type
									at0036,    -- Signet ring cell carcinoma
									at0038,    -- Adenosquamous carcinoma
									at0413,    -- Spindle cell carcinoma
									at0037,    -- Squamous cell carcinoma
									at0041,    -- Undifferentiated carcinoma
									at0040,    -- Neuroendocrine tumour G1
									at0414,    -- Neuroendocrine tumour G2
									at0418,    -- Neuroendocrine carcinoma
									at0415,    -- Neuroendocrine carcinoma, large cell
									at0416,    -- Neuroendocrine carcinoma, small cell
									at0417]    -- Mixed adenoneuroendocrine carcinoma
								}
							}
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0002] occurrences matches {0..1} matches {    -- pTNM classification
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.tnm_stage_pathological\.v0/}
					}
					ELEMENT[at0323] occurrences matches {0..1} matches {    -- Distance of invasion
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"mm">
										magnitude = <|>=0.0|>
									>
								>
							>
						}
					}
					CLUSTER[at0389] occurrences matches {0..1} matches {    -- Response to neoadjuvant therapy
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0390] occurrences matches {0..1} matches {    -- Description
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0003] occurrences matches {0..1} matches {    -- Details
								include
									archetype_id/value matches {/.*/}
							}
						}
					}
					CLUSTER[at0269] occurrences matches {0..1} matches {    -- Histological grading
						items cardinality matches {1..*; unordered} matches {
							allow_archetype Not_Set[at0391] occurrences matches {0..*} matches {    -- Grade
								include
									archetype_id/value matches {/.*/}
							}
							ELEMENT[at0271] occurrences matches {0..1} matches {    -- Comment
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					CLUSTER[at0030] occurrences matches {0..1} matches {    -- Lymph node findings
						items cardinality matches {1..*; unordered} matches {
							allow_archetype CLUSTER[at0087] occurrences matches {0..1} matches {    -- Lymph node detail
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.lymph_node_metastases\.v1/}
							}
							ELEMENT[at0365] occurrences matches {0..1} matches {    -- Status of apical lymph node
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0376,    -- Metastasis present
											at0377,    -- Metastasis absent
											at0378,    -- Equivocal
											at0379]    -- Indeterminate
										}
									}
								}
							}
							ELEMENT[at0348] occurrences matches {0..1} matches {    -- Comment
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					CLUSTER[at0403] occurrences matches {0..1} matches {    -- Tumour deposits
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0404] occurrences matches {0..1} matches {    -- Assessment of tumour deposits
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0405,    -- Present
											at0406,    -- Absent
											at0407,    -- Equivocal
											at0408]    -- Indeterminate
										}
									}
								}
							}
							ELEMENT[at0281] occurrences matches {0..1} matches {    -- Number of tumour deposits
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0380,    -- Not applicable
											at0388,    -- 0
											at0381,    -- 1
											at0382,    -- 2
											at0383,    -- 3
											at0384,    -- 4
											at0385,    -- 5
											at0386,    -- >5
											at0387,    -- Equivocal
											at0284]    -- Indeterminate
										}
									}
								}
							}
							ELEMENT[at0409] occurrences matches {0..1} matches {    -- Comment
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					CLUSTER[at0278] occurrences matches {0..1} matches {    -- Local tissue invasion
						items cardinality matches {1..*; unordered} matches {
							allow_archetype CLUSTER[at0374] occurrences matches {0..*} matches {    -- Large vessel (venous) invasion
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.tumour_invasion\.v1/}
							}
							allow_archetype CLUSTER[at0277] occurrences matches {0..*} matches {    -- Small vessel invasion
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.tumour_invasion\.v1/}
							}
							allow_archetype CLUSTER[at0286] occurrences matches {0..*} matches {    -- Perineural invasion
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.tumour_invasion\.v1/}
							}
						}
					}
					CLUSTER[at0263] occurrences matches {0..1} matches {    -- Distant metastasis
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0264] occurrences matches {0..1} matches {    -- Distant metastasis
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0362,    -- Present
											at0363,    -- Absent
											at0364]    -- Indeterminate
										}
									}
								}
							}
							allow_archetype CLUSTER[at0287] occurrences matches {0..*} matches {    -- Sites
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location\.v1/}
							}
							ELEMENT[at0265] occurrences matches {0..1} matches {    -- Description
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					CLUSTER[at0366] occurrences matches {0..*} matches {    -- Polyps
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0359] occurrences matches {0..1} matches {    -- Type of polyp
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0338] occurrences matches {0..1} matches {    -- Number of polyps
								value matches {
									DV_COUNT matches {
										magnitude matches {|>=0|}
									}
								}
							}
							ELEMENT[at0367] occurrences matches {0..1} matches {    -- Polyposis syndrome
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0368,    -- Present
											at0369,    -- Absent
											at0419,    -- Equivocal
											at0370]    -- Indeterminate
										}
									}
								}
							}
							ELEMENT[at0371] occurrences matches {0..1} matches {    -- Comment
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					CLUSTER[at0179] occurrences matches {0..1} matches {    -- Additional findings
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0250] occurrences matches {0..*} matches {    -- Additional finding
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0293,    -- Synchronous carcinoma
											at0294,    -- Ulcerative colitis
											at0295,    -- Ulcerative colitis with dysplasia
											at0296,    -- Crohn's disease
											at0297,    -- Crohn's disease with dysplasia
											at0340]    -- Diverticulosis
										}
									}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0220] occurrences matches {0..1} matches {    -- Description
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					ELEMENT[at0309] occurrences matches {0..1} matches {    -- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

