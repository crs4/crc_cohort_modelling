					text = <"Indeterminate">
					description = <"Presence of tumour perforation has not been determined.">
				>
				["at0018"] = <
					text = <"Present">
					description = <"Tumour perforation is present.">
				>
				["at0019"] = <
					text = <"Absent">
					description = <"Tumour perforation is absent.">
				>
				["at0020"] = <
					text = <"Retro/infra peritoneal">
					description = <"The perforation is retro/infra peritoneal.">
				>
				["at0021"] = <
					text = <"Serosal">
					description = <"The perforation is serosal.">
				>
				["at0022"] = <
					text = <"Entirely above">
					description = <"The tumour is entirely above the level of the peritoneal reflection anteriorly.">
				>
				["at0023"] = <
					text = <"Astride">
					description = <"The tumour is astride (or at)  the level of the peritoneal reflection anteriorly.">
				>
				["at0024"] = <
					text = <"Entirely below">
					description = <"The tumour is entirely below the level of the peritoneal reflection anteriorly.">
				>
				["at0025"] = <
					text = <"Incomplete">
					description = <"Little bulk of mesorectum, very irregular with defects down to the muscularis propria.">
				>
				["at0026"] = <
					text = <"Nearly complete">
					description = <"Moderate bulk of mesorectum. Irregular surface with defects greater than 5 mm, but not extending to the muscularis propria. Moderate degree of coning may be seen.">
				>
				["at0027"] = <
					text = <"Complete">
					description = <"A good bulk of mesorectum with a smooth surface. No coning near the tumor. No defects more than 5 mm deep.">
